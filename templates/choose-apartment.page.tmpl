{{template "base" .}}

{{define "content"}}
<div class="container">

    <div class="row">
        <div class="col">
            <h1 class="text-center"> Choose An Apartment</h1>

            {{$apartments := index .Data "apartments"}}
            <ul>
                {{range $apartments}}
                    <li><a href="/choose-apartment/{{.ID}}">{{.ApartmentName}}</a></li>
                {{end}}
            </ul>
        </div>
    </div>
</div>
   
{{end}}